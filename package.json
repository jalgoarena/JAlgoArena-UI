{
  "name": "jalgoarena-ui",
  "version": "2.4.0",
  "description": "JAlgoArena UI",
  "main": "build/server.js",
  "scripts": {
    "installRequiredGlobalDependencies": "npm install webpack-cli -g",
    "test": "jest && codecov",
    "test:watch": "jest --watch",
    "build": "webpack --config webpack.server.config.js --mode=production && webpack --config webpack.client.config.js --mode=production",
    "prestart": "npm run build",
    "start": "node build/server.js",
    "predeploy": "npm run build",
    "deploy": "node createReleasePackage.js",
    "client": "webpack-dev-server --mode=development --devtool inline-source-map",
    "devBuild": "webpack --config webpack.server.config.js --mode=development && webpack --config webpack.client.config.js --mode=development",
    "predev": "npm run devBuild",
    "dev": "node build/server.js",
    "dev:watch": "nodemon -e js,jsx --watch client --watch server --exec npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jalgoarena/JAlgoArena-UI.git"
  },
  "keywords": [
    "JAlgoArena"
  ],
  "author": "Jacek Sp√≥lnik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jalgoarena/JAlgoArena-UI/issues"
  },
  "homepage": "https://github.com/jalgoarena/JAlgoArena-UI#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0-10",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.8",
    "@stomp/stompjs": "^4.0.2",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.11",
    "@types/react-bootstrap": "^0.32.11",
    "@types/react-bootstrap-table": "^4.3.5",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-transition-group": "^2.0.13",
    "@types/redux": "^3.6.0",
    "@types/remarkable": "^1.7.0",
    "@types/sockjs-client": "^1.1.0",
    "ajv": "^6.4.0",
    "archiver": "^2.1.1",
    "bootstrap": "^3.3.7",
    "brace": "^0.11.1",
    "connected-react-router": "^4.3.0",
    "es6-promise": "^4.2.4",
    "highcharts": "^6.1.1",
    "highcharts-react-official": "^1.3.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-ace": "^5.10.0",
    "react-addons-create-fragment": "^15.6.2",
    "react-blockies": "^1.3.0",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table": "^4.3.1",
    "react-calendar-heatmap": "^1.6.3",
    "react-dom": "^16.3.2",
    "react-mixins": "^0.1.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.7.1",
    "s": "^0.1.1",
    "sockjs-client": "^1.1.4",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@types/highcharts": "^5.0.26",
    "@types/react-calendar-heatmap": "^1.6.2",
    "babel-jest": "^23.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-root-element-plugin": "^0.1.3",
    "jest": "^23.1.0",
    "jest-fetch-mock": "^1.5.0",
    "jest-localstorage-mock": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redux-mock-store": "^1.2.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-parse": "latest",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "automock": false,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFiles": [
      "jest-localstorage-mock",
      "./setupJest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
